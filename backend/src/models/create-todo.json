{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "create-todo",
    "type": "object",
    "properties": {
      "todoId": {
          "type": "string"
      },
      "userId": {
          "type": "string"
      },
      "createdAt": {
        "type": "string"
      },
      "name": {
          "type": "string",
          "pattern": "^(?!\\s*$).+"
      },
      "dueDate": {
        "type": "string"
      },
      "done":{
        "type": "boolean"
      },
      "attachmentUrl":{
        "type": "string"
      }
    },
    "required": [  
      "name"
    ],
    "additionalProperties": false
  }