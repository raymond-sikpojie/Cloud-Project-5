{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "update-todo",
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "pattern": "^(?!\\s*$).+"
      },
      "dueDate": {
        "type": "string"
      },
      "done":{
        "type": "boolean"
      }
    },
    "required": [
      "name",
      "dueDate",
      "done"
    ],
    "additionalProperties": false
  }